config {
  type: "table",
  schema: 'bbb_24',

  bigquery: {
    labels: {
      schedule: "diario",
      tags: "bbb_24",
      owner: "martech"
    }
  },
  description: 'Tabela com os usu치rios do raiox_bbb_24_assinantes e a semana em que entraram no cluster.',
  columns: {
    globo_id: 'C칩digo que identifica o usu치rio.',
    min_semana: 'Semana em que o usu치rio entrou na base raiox_bbb_24_assinantes.'
  }

}

WITH entrada_cluster AS (
  SELECT
    DISTINCT globo_id,
    MIN(semana) AS min_semana
  FROM
    ${ref("bbb_24", "raiox_bbb_24_assinantes")}
  GROUP BY
    1
)
SELECT
  *
FROM
  entrada_cluster
